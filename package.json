{
  "name": "photography-backend",
  "version": "1.0.0",
  "description": "API server for Masha's Photography Website.",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon index.js",
    "client": "babel-node start-client.js",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=lcov _mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": "https://github.com/rockchalkwushock/photography-backend.git",
  "keywords": [
    "bcrypt",
    "dotenv",
    "express",
    "passport",
    "passport-local",
    "passport-jwt"
  ],
  "author": "Cody Brunner <rockchalkwushock@icloud.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rockchalkwushock/photography-backend/issues"
  },
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "bcrypt": "^1.0.1",
    "body-parser": "^1.15.2",
    "chai": "^3.5.0",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "faker": "^3.1.0",
    "jsonwebtoken": "^7.2.1",
    "lodash": "^4.17.4",
    "mongoose": "^4.7.5",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "supertest": "^2.0.1",
    "validator": "^6.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-plugin-istanbul": "^3.0.0",
    "concurrently": "^3.1.0",
    "coveralls": "^2.11.15",
    "cross-env": "^3.1.3",
    "eslint": "^3.12.2",
    "eslint-config-rallycoding": "^3.1.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "nyc": "^10.0.0"
