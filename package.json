  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "build": "cross-env yarn run clean && NODE_ENV=production webpack -p --progress --profile --colors --config webpack.prod.config.js",
    "clean": "rimraf dist npm-debug.log.*",
    "dev:c": "cross-env NODE_ENV=development webpack-dev-server --colors --profile --config webpack.dev.config.js",
    "dev:s": "cross-env NODE_ENV=development nodemon index.js",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=lcov _mocha",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "engines": {
    "node": "7.2.1"
  },
